{
    "name": "sigrun/systim",
    "description": "PHP Wrapper for Systim API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Ognicki",
            "email": "marek@sigrun.eu"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "danielstjules/stringy": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Systim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Systim\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "phpunit/phpcov": "^6.0",
        "php-coveralls/php-coveralls": "^2.1",
        "vlucas/phpdotenv": "^3.6",
        "phpunit/php-code-coverage": "^7.0"
    }
}
